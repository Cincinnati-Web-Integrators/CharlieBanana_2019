          <label for="contactFormPhone">{{ 'contact.form.phone' | t }}</label>
          <input type="tel" id="contactFormPhone" name="contact[phone]" placeholder="{{ 'contact.form.phone' | t }}" value="{% if form.phone %}{{ form.phone }}{% elsif customer %}{{ customer.phone }}{% endif %}" required>
		    
          <label for="contactFormEmail">{{ 'contact.form.email' | t }}</label>
          <input type="email" id="contactFormEmail" name="contact[email]" placeholder="{{ 'contact.form.email' | t }}" autocorrect="off" autocapitalize="off" value="{% if form.email %}{{ form.email }}{% elsif customer %}{{ customer.email }}{% endif %}">

          <label for="contactFormWhatReturning">What are you returning?</label>
          <textarea rows="10" id="contactFormWhatReturning" name="contact[what_returning]">{% if form.what_returning %}{{ form.what_returning }}{% endif %}</textarea>
<br />
<label>Reasons for Return</label>
  <select id="contactFormReasons" name="contact[reasons]" required>
    <option value="Wrong Product Shipped">Wrong Product Shipped</option>
    <option value="Product Missing">Product Missing</option>
	<option value="Duplicate Order">Duplicate Order</option>
	<option value="Defective Product">Defective Product</option>
	<option value="Other... Explain below">Other... Explain below</option>				
  </select>
  <br />
<label>What can we do?</label>
  <select id="contactFormRWhatCanWeDo" name="contact[what_can_we_do]" required>
    <option value="Exchange">Exchange</option>
    <option value="Replacement">Replacement</option>
	<option value="Store Credit">Store Credit</option>
	<option value="Refund">Refund</option>
	<option value="Send Missing Product">Send Missing Product</option>				
	<option value="Other... Explain below">Other... Explain below</option>				
  </select>  
<br />
          <label for="contactFormMessage">Attachment(s)</label>
          <input type="file" name="contact[attachment_file]" multiple />
<br />
          <label for="contactFormMessage">Other Details / Notes</label>
          <textarea rows="10" id="contactFormMessage" name="contact[body]">{% if form.body %}{{ form.body }}{% endif %}</textarea>
	<!--
